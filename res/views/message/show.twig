		<div class="col-md-6 col-md-offset-3">
			<div class="panel panel-default">
				<div class="panel-body">
					{% if m.message %}
						{{ m.message }}
					{% else %}
						Message already read!
					{% endif %}
				</div>
			</div>
		</div>
	</div>
{% endblock %}